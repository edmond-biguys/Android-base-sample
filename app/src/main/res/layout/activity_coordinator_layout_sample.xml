<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    app:divider="@drawable/shape_div_line"
    app:dividerPadding="20dp"
    app:showDividers="middle"
    android:orientation="vertical"
    tools:context="com.cym.androidx.coordinatorlayout.AboutBehaviorActivity">


  <TextView
      android:id="@+id/tv_behavior"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:text="behavior"
      android:textColor="#000000"
      android:gravity="center"
      />

  <TextView
      android:id="@+id/tv_appBarLayout"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:text="appBarLayout"
      android:textColor="#000000"
      android:gravity="center"
      />

  <TextView
      android:id="@+id/tv_collapsingToolbarLayout"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:text="collapsingToolbarLayout"
      android:textColor="#000000"
      android:gravity="center"
      />

</androidx.appcompat.widget.LinearLayoutCompat>